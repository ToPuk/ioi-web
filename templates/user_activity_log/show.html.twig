{% extends 'base.html.twig' %}

{% block title %}UserActivityLog{% endblock %}

{% block body %}
    <h1>UserActivityLog</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ user_activity_log.id }}</td>
            </tr>
            <tr>
                <th>ActivityType</th>
                <td>{{ user_activity_log.activityType }}</td>
            </tr>
            <tr>
                <th>Data</th>
                <td>{{ user_activity_log.data ? user_activity_log.data|json_encode : '' }}</td>
            </tr>
            <tr>
                <th>ActivityDate</th>
                <td>{{ user_activity_log.activityDate ? user_activity_log.activityDate|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
            <tr>
                <th>UserId</th>
                <td>{{ user_activity_log.userId }}</td>
            </tr>
            <tr>
                <th>UserEmail</th>
                <td>{{ user_activity_log.userEmail }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('user_activity_log_index') }}">back to list</a>

    <a href="{{ path('user_activity_log_edit', {'id': user_activity_log.id}) }}">edit</a>

